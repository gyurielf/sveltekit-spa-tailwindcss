{
	"name": "sveltekit-true-spa-demo",
	"version": "0.0.1",
	"engines": {
		"npm": ">= 7",
		"yarn": "please-use-npm",
		"node": ">= 16"
	},
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"svelte-check": "svelte-check --ignore 'dist,build,coverage,.svelte-kit' --threshold warning",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"lint:fix": "npm run eslint:fix && npm run stylelint:fix && npm run prettier:fix",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"prettier": "prettier --check --plugin-search-dir=. .",
		"prettier:fix": "prettier --write --plugin-search-dir=. .",
		"test": "jest --passWithNoTests",
		"test:watch": "npm test -- --watch"
	},
	"dependencies": {
		"@lukeed/uuid": "^2.0.0",
		"js-cookie": "^3.0.1"
	},
	"devDependencies": {
		"@babel/core": "^7.15.5",
		"@babel/preset-env": "^7.15.6",
		"@sveltejs/adapter-static": "^1.0.0-next.20",
		"@sveltejs/kit": "next",
		"@testing-library/jest-dom": "^5.14.1",
		"@types/jest": "^27.0.2",
		"@types/js-cookie": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^4.32.0",
		"@typescript-eslint/parser": "^4.32.0",
		"autoprefixer": "^10.3.6",
		"dotenv": "^10.0.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"jest": "^27.2.4",
		"postcss": "^8.3.8",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"sass": "^1.42.1",
		"svelte": "^3.43.1",
		"svelte-check": "^2.2.6",
		"svelte-jester": "^2.1.5",
		"svelte-preprocess": "^4.9.5",
		"ts-jest": "^27.0.5",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3",
		"postcss-load-config": "^3.1.0",
		"cssnano": "^5.0.8",
		"tailwindcss": "^2.2.16"
	},
	"type": "module",
	"license": "MIT"
}